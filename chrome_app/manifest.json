{
  "name": "Frankie Chrome Extension",
  "version": "1.0",
  "description": "Uses Frankie <Link> to check if a visited website contains misinformation.",
  "permissions": ["activeTab", "declarativeContent", "storage", "*://*/*"],
  "background": {
    "scripts": ["node_modules/jquery/dist/jquery.js", "js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "web_accessible_resources": [
    "images/*.png", "popup.html", "modal.html", "css/bootstrap.min.css"
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_scripts" : [
    {
      "matches" : ["<all_urls>"],
      "css" : ["css/bootstrap.min.css"],
      "js": ["js/jquery.min.js", "js/parser.js"]
    }
  ],
  "manifest_version": 2
}